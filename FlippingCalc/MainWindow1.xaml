<Window x:Class="FlippingCalc.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlippingCalc"
        mc:Ignorable="d"
        Title="FlippingCalc" Height="790" Width="700" 
        Icon="Images/Background/CurrencyAddModToRare.png" 
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
        TextElement.FontWeight="Medium"
        TextElement.FontSize="14"
        FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto"
        Background="{DynamicResource MaterialDesignPaper}"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}">
    <Window.Resources>
        <Style x:Key="TextBlockStyle1" TargetType="{x:Type TextBlock}">
            <Setter Property="TextWrapping" Value="NoWrap"/>
            <Setter Property="TextTrimming" Value="None"/>
        </Style>
    </Window.Resources>
    <TabControl>
        <TabControl.Background>
            <ImageBrush ImageSource="Images\Background\untitled-1.png" />
        </TabControl.Background>
        <TabItem Header="Currency">
                <StackPanel Height="720">
                    <Viewbox Stretch="Uniform"  HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <Grid >
                            <!-- Column Def -->
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1.5*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1*" />
                                <ColumnDefinition Width="1.5*" />
                            </Grid.ColumnDefinitions>
                            <!-- Row Def -->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                            <!-- Top Row -->
                            <!-- left -->
                            <Rectangle Grid.Column="0" Grid.Row="0" Height="30" Fill="DodgerBlue" Margin="10 0 10 0" Stroke="Black"/>
                            <Rectangle Grid.Column="1" Grid.Row="0" Height="30" Fill="Yellow" Stroke="Black" Margin="0 0 10 0"/>
                            <Rectangle Grid.Column="2" Grid.Row="0" Height="30" Fill="Yellow" Stroke="Black" Margin="0 0 10 0"/>
                            <Rectangle Grid.Column="3" Grid.Row="0" Height="30" Fill="SeaGreen" Margin="0 0 0 0" Stroke="Black"/>
                            <!-- right -->
                            <Rectangle Grid.Column="4" Grid.Row="0" Height="30" Fill="DodgerBlue" Margin="10 0 10 0" Stroke="Black"/>
                            <Rectangle Grid.Column="5" Grid.Row="0" Height="30" Fill="Yellow" Stroke="Black" Margin="0 0 10 0"/>
                            <Rectangle Grid.Column="6" Grid.Row="0" Height="30" Fill="Yellow" Stroke="Black" Margin="0 0 10 0"/>
                            <Rectangle Grid.Column="7" Grid.Row="0" Height="30" Fill="SeaGreen" Margin="0 0 0 0" Stroke="Black"/>
                            <!-- left -->
                            <TextBlock Grid.Column="0" Grid.Row="0" HorizontalAlignment="Center" Text="TYPE" VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Margin="10,0,10,0" Foreground="#DD000000"/>
                            <TextBlock Grid.Column="1" Grid.Row="0" Text=" ⮂BUY " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Margin="0,0,10,0" HorizontalAlignment="Center" Style="{DynamicResource TextBlockStyle1}" Foreground="#DD000000"/>
                            <TextBlock Grid.Column="2" Grid.Row="0" Text=" ⮀SELL " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" Margin="0,0,10,0" HorizontalAlignment="Center" Foreground="#DD000000" />
                            <TextBlock Grid.Column="3" Grid.Row="0" HorizontalAlignment="Center" Text=" PROFIT " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Foreground="#DD000000" Background="#FF3AFF00"/>
                            <!-- right -->
                            <TextBlock Grid.Column="4" Grid.Row="0" HorizontalAlignment="Center" Text="TYPE" VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Foreground="#DD000000"/>
                            <TextBlock Grid.Column="5" Grid.Row="0" Text=" ⮂BUY " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Margin="0,0,10,0" HorizontalAlignment="Center" Foreground="#DD000000"/>
                            <TextBlock Grid.Column="6" Grid.Row="0" Text=" ⮀SELL " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" Margin="0,0,10,0" HorizontalAlignment="Center" Foreground="#DD000000" />
                            <TextBlock Grid.Column="7" Grid.Row="0" HorizontalAlignment="Center" Text=" PROFIT " VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="22" FontWeight="Bold" TextDecorations="{x:Null}" Foreground="#DD000000" Background="#FF0CFF00"/>
                            <!-- Row 1 -->
                            <!-- left -->
                            <Image Grid.Column="0" Grid.Row="1" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Alteration.png" ToolTip="Orb of Alteration"/>
                            <TextBox x:Name="BuyAlt" Grid.Row="1" Grid.Column="1" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellAlt" Grid.Row="1" Grid.Column="2" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitAlt" Grid.Row="1" Grid.Column="3" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                            <Image Grid.Column="4" Grid.Row="1" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Engineers.png" ToolTip="Engineer's Orb"/>
                        <TextBox x:Name="BuyEngineers" Grid.Column="5" Grid.Row="1" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellEngineers" Grid.Column="6" Grid.Row="1" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitEngineers" Grid.Column="7" Grid.Row="1" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 2 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="2" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Alchemy.png" ToolTip="Orb of Alchemy"/>
                            <TextBox x:Name="BuyAlchemy" Grid.Row="2" Grid.Column="1" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellAlchemy" Grid.Row="2" Grid.Column="2" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitAlchemy" Grid.Row="2" Grid.Column="3" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="2" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Fusing.png" ToolTip="Orb of Fusing"/>
                        <TextBox x:Name="BuyFusing" Grid.Column="5" Grid.Row="2" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellFusing" Grid.Column="6" Grid.Row="2" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitFusing" Grid.Column="7" Grid.Row="2" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 3 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="3" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Exalted.png" ToolTip="Exalted Orb"/>
                            <TextBox x:Name="BuyEx" Grid.Column="1" Grid.Row="3" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellEx" Grid.Column="2" Grid.Row="3" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitEx" Grid.Column="3" Grid.Row="3" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="3" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/GCP.png" ToolTip="Gemcutter's Prism"/>
                        <TextBox x:Name="BuyGCP" Grid.Row="3" Grid.Column="5" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellGCP" Grid.Row="3" Grid.Column="6" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitGCP" Grid.Row="3" Grid.Column="7" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 4 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="4" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Jewellers.png" ToolTip="Jeweller's orb"/>
                            <TextBox x:Name="BuyJeweller" Grid.Column="1" Grid.Row="4" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellJeweller" Grid.Column="2" Grid.Row="4" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitJeweller" Grid.Column="3" Grid.Row="4" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="4" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Chromatic.png" ToolTip="Chromatic Orb"/>
                        <TextBox x:Name="BuyChromatic" Grid.Column="5" Grid.Row="4" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellChromatic" Grid.Column="6" Grid.Row="4" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitChromatic" Grid.Column="7" Grid.Row="4" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 5 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="5" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Chisel.png" ToolTip="Cartographer's Chisel"/>
                            <TextBox x:Name="BuyChisel" Grid.Column="1" Grid.Row="5" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellChisel" Grid.Column="2" Grid.Row="5" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitChisel" Grid.Column="3" Grid.Row="5" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="5" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Chance.png" ToolTip="Orb of Chance"/>
                        <TextBox x:Name="BuyChance" Grid.Column="5" Grid.Row="5" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellChance" Grid.Column="6" Grid.Row="5" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitChance" Grid.Column="7" Grid.Row="5" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 6 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="6" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Blessed.png" ToolTip="Blessed Orb"/>
                            <TextBox x:Name="BuyBlessed" Grid.Column="1" Grid.Row="6" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellBlessed" Grid.Column="2" Grid.Row="6" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitBlessed" Grid.Column="3" Grid.Row="6" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="6" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Scouring.png" ToolTip="Orb of Scouring"/>
                        <TextBox x:Name="BuyScouring" Grid.Column="5" Grid.Row="6" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellScouring" Grid.Column="6" Grid.Row="6" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitScouring" Grid.Column="7" Grid.Row="6" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 7 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="7" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Regal.png" ToolTip="Regal Orb"/>
                            <TextBox x:Name="BuyRegal" Grid.Column="1" Grid.Row="7" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellRegal" Grid.Column="2" Grid.Row="7" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitRegal" Grid.Column="3" Grid.Row="7" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="7" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Regret.png" ToolTip="Orb of Regret"/>
                        <TextBox x:Name="BuyRegret" Grid.Column="5" Grid.Row="7" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellRegret" Grid.Column="6" Grid.Row="7" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitRegret" Grid.Column="7" Grid.Row="7" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 8 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="8" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Vaal.png" ToolTip="Vaal Orb"/>
                            <TextBox x:Name="BuyVaal" Grid.Column="1" Grid.Row="8" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellVaal" Grid.Column="2" Grid.Row="8" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitVaal" Grid.Column="3" Grid.Row="8" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="8" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Divine.png" ToolTip="Divine Orb"/>
                        <TextBox x:Name="BuyDivine" Grid.Column="5" Grid.Row="8" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellDivine" Grid.Column="6" Grid.Row="8" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitDivine" Grid.Column="7" Grid.Row="8" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 9 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="9" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Portal.png" ToolTip="Portal Scroll"/>
                            <TextBox x:Name="BuyPortal" Grid.Column="1" Grid.Row="9" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellPortal" Grid.Column="2" Grid.Row="9" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitPortal" Grid.Column="3" Grid.Row="9" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="9" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Wisdom.png" ToolTip="Scroll of Wisdom"/>
                        <TextBox x:Name="BuyWisdom" Grid.Column="5" Grid.Row="9" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellWisdom" Grid.Column="6" Grid.Row="9" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitWisdom" Grid.Column="7" Grid.Row="9" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 10 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="10" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Whetstone.png" ToolTip="Blacksmith's Whetstone"/>
                            <TextBox x:Name="BuyWhetstone" Grid.Column="1" Grid.Row="10" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellWhetstone" Grid.Column="2" Grid.Row="10" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitWhetstone" Grid.Column="3" Grid.Row="10" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="10" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Scrap.png" ToolTip="Armourer's Scrap"/>
                        <TextBox x:Name="BuyScrap" Grid.Column="5" Grid.Row="10" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellScrap" Grid.Column="6" Grid.Row="10" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitScrap" Grid.Column="7" Grid.Row="10" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 11 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="11" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Transmute.png" ToolTip="Orb of Transmutation"/>
                            <TextBox x:Name="BuyTransmute" Grid.Column="1" Grid.Row="11" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellTransmute" Grid.Column="2" Grid.Row="11" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitTransmute" Grid.Column="3" Grid.Row="11" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
                        <Image Grid.Column="4" Grid.Row="11" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Bauble.png" ToolTip="Glassblower's Bauble"/>
                        <TextBox x:Name="BuyBauble" Grid.Column="5" Grid.Row="11" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellBauble" Grid.Column="6" Grid.Row="11" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitBauble" Grid.Column="7" Grid.Row="11" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- Row 12 -->
                            <!-- left -->
                        <Image Grid.Column="0" Grid.Row="12" HorizontalAlignment="center" Height="35" Margin="0 0 0 0" VerticalAlignment="center" Width="100" Source="Images/Orbs/Perandus.png" ToolTip="Perandus Coin"/>
                        <TextBox x:Name="BuyPerandus" Grid.Column="1" Grid.Row="12" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                        <TextBox x:Name="SellPerandus" Grid.Column="2" Grid.Row="12" Height="25" Text="" Margin="0 0 10 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" MaxLength="6" Foreground="White"/>
                            <TextBox x:Name="ProfitPerandus" Grid.Column="3" Grid.Row="12" Height="25" Text="" Margin="0 0 0 0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" IsReadOnly="True" Background="DarkGray" Foreground="#DD000000"/>
                            <!-- right -->
